<html>
  <head>
    <title><%= poll.title %></title>
  </head>
  <body>
    <div>
      <h2>Poll title: <%= poll.title %></h2>
      <h3>Poll Results:</h3>
      <% for (var i = 0; i < poll.responses.length; i++) { %>
      <p id='response-<%= i %>'><%= poll.responses[i] %></p>
      <% } %>
    </div>
    <div id="poll-results"></div>
    <script src="/socket.io/socket.io.js"></script>
    <script src="/client.js"></script>
  </body>
</html>
